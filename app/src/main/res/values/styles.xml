<resources>

    <style name="Lime" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimaryL</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkL</item>
        <item name="colorAccent">@color/colorAccentL</item>

        <item name="themeHeaderTextColor">@color/colorMainTextL</item>
        <item name="themeSecondTextColor">@color/colorSecondTextL</item>
        <item name="themeThirdTextColor">@color/colorThirdTextL</item>
        <item name="themeHintTextColor">@color/colorHintTextL</item>
        <item name="themeSearchTextColor">@color/colorSearchTextL</item>

        <!--Sign Fragment-->
        <item name="themeSignFieldsBGStartColor">@color/colorSignFieldStartL</item>
        <item name="themeSignFieldsBGEndColor">@color/colorSignFieldEndL</item>
        <item name="themeSignFieldsBG">@drawable/bg_sign_fields</item>

        <item name="themeSignButtonsBG">@drawable/bg_white_circle</item>
        <item name="themeSignHeaderTextColor">@color/colorSignHeaderL</item>
        <item name="themeSignHintTextColor">@color/colorSignHintL</item>
        <item name="themeSignFieldsTextColor">@color/colorSignTextL</item>
        <item name="themeSignButtonInBgColor">@color/colorSignButtonInBgL</item>
        <item name="themeSignButtonUpBgColor">@color/colorSignButtonUpBgL</item>
        <item name="themeSignButtonTextColor">@color/colorSignButtonTextL</item>

        <!--Main fragment-->
        <item name="themeMainSearchTextColor">@color/colorMainSearchTextL</item>
        <item name="themeMainSearchBG">@drawable/bg_sign_fields</item>
        <item name="themeMainSearchItemsColor">@color/colorMainSearchItemTextL</item>
        <item name="themeMainFABColor">@color/colorMainFabL</item>
        <item name="themeMainFABImageColor">@color/colorMainFabImageL</item>

        <!--Create fragment-->
        <item name="themeCreatorButtonCreateColor">@color/colorCreatorCreateBgL</item>
        <item name="themeCreatorButtonCancelColor">@color/colorCreatorCancelBgL</item>

        <!--Item Fragment-->
        <item name="themeItemFRStartDateColor">@color/colorItemFRStartDateL</item>
        <item name="themeItemSettingButtonBG">@drawable/bg_white_circle</item>
        <item name="themeItemSettingButtonBGTint">@color/colorItemFRSettingButtonBgL</item>
        <item name="themeItemRecyclerBG">@drawable/bg_white_rectangle</item>
        <item name="themeItemRecyclerBGTint">@color/transparent</item>
        <item name="themeItemButtonsBG">@drawable/bg_white_circle</item>
        <item name="themeItemButtonsBGTint">@color/colorItemFRButtonsBgL</item>
        <item name="themeItemFRProgressOnColor">@color/colorItemProgressOnL</item>
        <item name="themeItemFRProgressOffColor">@color/colorItemProgressOffL</item>

        <!--Item Rows-->
        <item name="themeItemRowBG">@drawable/bg_white_rectangle</item>
        <item name="themeItemRowBGTint">@color/colorItemRowBgTintL</item>
        <item name="themeItemSmallRowBG">@drawable/bg_white_rectangle</item>
        <item name="themeItemSmallRowBGTint">@color/colorItemSmallRowBgTintL</item>
        <item name="themeItemRowActionButton">@drawable/bg_white_rectangle</item>
        <item name="themeItemRowActionButtonTint">@color/colorItemRowActButtonBgTintL</item>
    </style>

    <style name="Blue" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="themeHeaderTextColor">@color/colorMainText</item>
        <item name="themeSecondTextColor">@color/colorSecondText</item>
        <item name="themeThirdTextColor">@color/colorThirdText</item>
        <item name="themeHintTextColor">@color/colorHintText</item>

        <!--Sign Fragment-->
        <item name="themeSignFieldsBGStartColor">@color/colorSignFieldStart</item>
        <item name="themeSignFieldsBGEndColor">@color/colorSignFieldEnd</item>
        <item name="themeSignFieldsBG">@drawable/bg_sign_fields</item>

        <item name="themeSignHeaderTextColor">@color/colorSignHeader</item>
        <item name="themeSignHintTextColor">@color/colorSignHint</item>
        <item name="themeSignFieldsTextColor">@color/colorSignText</item>
        <item name="themeSignButtonInBgColor">@color/colorSignButtonInBg</item>
        <item name="themeSignButtonUpBgColor">@color/colorSignButtonUpBg</item>
        <item name="themeSignButtonTextColor">@color/colorSignButtonText</item>

        <!--Main fragment-->
        <item name="themeMainSearchTextColor">@color/colorMainSearchText</item>
        <item name="themeMainSearchBG">@drawable/bg_sign_fields</item>
        <item name="themeMainSearchItemsColor">@color/colorMainSearchItemText</item>
        <item name="themeMainFABColor">@color/colorMainFab</item>
        <item name="themeMainFABImageColor">@color/colorMainFabImage</item>

        <!--Create fragment-->
        <item name="themeCreatorButtonCreateColor">@color/colorCreatorCreateBg</item>
        <item name="themeCreatorButtonCancelColor">@color/colorCreatorCancelBg</item>

        <!--Item Fragment-->
        <item name="themeItemFRStartDateColor">@color/colorItemFRStartDate</item>
        <item name="themeItemSettingButtonBG">@drawable/bg_white_circle</item>
        <item name="themeItemSettingButtonBGTint">@color/colorItemFRSettingButtonBg</item>
        <item name="themeItemRecyclerBG">@drawable/bg_white_rectangle</item>
        <item name="themeItemRecyclerBGTint">@color/transparent</item>
        <item name="themeItemButtonsBG">@drawable/bg_white_circle</item>
        <item name="themeItemButtonsBGTint">@color/colorItemFRButtonsBg</item>
        <item name="themeItemFRProgressOnColor">@color/colorItemProgressOn</item>
        <item name="themeItemFRProgressOffColor">@color/colorItemProgressOff</item>

        <!--Item Rows-->
        <item name="themeItemRowBG">@drawable/bg_white_rectangle</item>
        <item name="themeItemRowBGTint">@color/colorItemRowBgTint</item>
        <item name="themeItemSmallRowBG">@drawable/bg_white_rectangle</item>
        <item name="themeItemSmallRowBGTint">@color/colorItemSmallRowBgTint</item>
        <item name="themeItemRowActionButton">@drawable/bg_white_rectangle</item>
        <item name="themeItemRowActionButtonTint">@color/colorItemRowActButtonBgTint</item>
    </style>

    <style name="Pink" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryP</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkP</item>
        <item name="colorAccent">@color/colorAccentP</item>

        <item name="themeHeaderTextColor">@color/colorMainTextP</item>
        <item name="themeSecondTextColor">@color/colorSecondTextP</item>
        <item name="themeThirdTextColor">@color/colorThirdTextP</item>
        <item name="themeHintTextColor">@color/colorHintTextP</item>

        <!--Sign Fragment-->
        <item name="themeSignFieldsBGStartColor">@color/colorSignFieldStartP</item>
        <item name="themeSignFieldsBGEndColor">@color/colorSignFieldEndP</item>
        <item name="themeSignFieldsBG">@drawable/bg_sign_fields</item>

        <item name="themeSignHeaderTextColor">@color/colorSignHeaderP</item>
        <item name="themeSignHintTextColor">@color/colorSignHintP</item>
        <item name="themeSignFieldsTextColor">@color/colorSignTextP</item>
        <item name="themeSignButtonInBgColor">@color/colorSignButtonInBgP</item>
        <item name="themeSignButtonUpBgColor">@color/colorSignButtonUpBgP</item>
        <item name="themeSignButtonTextColor">@color/colorSignButtonTextP</item>

        <!--Main fragment-->
        <item name="themeMainSearchTextColor">@color/colorMainSearchTextP</item>
        <item name="themeMainSearchBG">@drawable/bg_sign_fields</item>
        <item name="themeMainSearchItemsColor">@color/colorMainSearchItemTextP</item>
        <item name="themeMainFABColor">@color/colorMainFabP</item>
        <item name="themeMainFABImageColor">@color/colorMainFabImageP</item>

        <!--Create fragment-->
        <item name="themeCreatorButtonCreateColor">@color/colorCreatorCreateBgP</item>
        <item name="themeCreatorButtonCancelColor">@color/colorCreatorCancelBgP</item>

        <!--Item Fragment-->
        <item name="themeItemFRStartDateColor">@color/colorItemFRStartDateP</item>
        <item name="themeItemSettingButtonBG">@drawable/bg_white_circle</item>
        <item name="themeItemSettingButtonBGTint">@color/colorItemFRSettingButtonBgP</item>
        <item name="themeItemRecyclerBG">@drawable/bg_white_rectangle</item>
        <item name="themeItemRecyclerBGTint">@color/transparent</item>
        <item name="themeItemButtonsBG">@drawable/bg_white_circle</item>
        <item name="themeItemButtonsBGTint">@color/colorItemFRButtonsBgP</item>
        <item name="themeItemFRProgressOnColor">@color/colorItemProgressOnP</item>
        <item name="themeItemFRProgressOffColor">@color/colorItemProgressOffP</item>

        <!--Item Rows-->
        <item name="themeItemRowBG">@drawable/bg_white_rectangle</item>
        <item name="themeItemRowBGTint">@color/colorItemRowBgTint</item>
        <item name="themeItemSmallRowBG">@drawable/bg_white_rectangle</item>
        <item name="themeItemSmallRowBGTint">@color/colorItemSmallRowBgTint</item>
        <item name="themeItemRowActionButton">@drawable/bg_white_rectangle</item>
        <item name="themeItemRowActionButtonTint">@color/colorItemRowActButtonBgTint</item>
    </style>

    <style name="Toolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:text">?themeSearchTextColor</item>
        <item name="android:background">?colorPrimary</item>
    </style>

    <style name="WrapContent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="FillSpace">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="AppEditText" parent="Widget.AppCompat.EditText">
        <item name="android:textColorHint">?themeHintTextColor</item>
        <item name="android:inputType">text</item>
        <item name="android:textColor">?themeSecondTextColor</item>
    </style>

    <style name="AppSecondText" parent="TextAppearance.AppCompat">
        <item name="android:textColor">?themeSecondTextColor</item>
    </style>

    <style name="AppThirdText" parent="TextAppearance.AppCompat">
        <item name="android:textColor">?themeThirdTextColor</item>
    </style>

    <style name="AppMainText" parent="TextAppearance.AppCompat">
        <item name="android:textColor">?attr/themeHeaderTextColor</item>
    </style>

    <style name="AppScreen">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:padding">@dimen/item_fragment_layout_padding</item>
    </style>

    <!-- Setting Fragment-->
    <style name="Setting" parent="AppMainText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/sign_info_text_size</item>
    </style>

    <style name="Setting.RadioGroup">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">@dimen/item_fragment_margin_between_blocks</item>
        <item name="android:orientation">vertical</item>
        <item name="android:textSize">@dimen/sign_info_text_size</item>
    </style>

    <style name="Setting.SignOutButton">
        <item name="android:layout_marginStart">@dimen/item_fragment_margin_between_blocks</item>
        <item name="android:src">@drawable/ic_exit</item>
        <item name="android:background">@color/transparent</item>
    </style>

    <style name="Setting.RadioItem">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/sign_info_text_size</item>
        <item name="android:textColor">?themeHeaderTextColor</item>
    </style>

    <!-- Sign Fragment-->
    <style name="Sign.Text.Info" parent="AppSecondText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/sign_info_text_size</item>
        <item name="android:text">@string/sign_signin_or_register</item>
    </style>

    <style name="Sign.Fields" parent="Widget.AppCompat.EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/sign_field_height</item>
        <item name="android:background">?themeSignFieldsBG</item>
        <item name="android:textColor">?themeSignFieldsTextColor</item>
        <item name="android:textStyle">italic</item>
        <item name="android:paddingStart">@dimen/sign_field_text_start_padding</item>
        <item name="android:textSize">@dimen/sign_field_text_size</item>
    </style>

    <style name="Sign.Fields.Login">
        <item name="android:inputType">textEmailAddress</item>
        <item name="android:layout_marginTop">@dimen/sign_info_text_margin_bottom</item>
        <item name="android:layout_marginEnd">@dimen/sign_field_login_margin_end</item>
        <item name="android:hint">@string/email_mail_com</item>
    </style>

    <style name="Sign.Fields.Password">
        <item name="android:inputType">textPassword</item>
        <item name="android:layout_marginTop">@dimen/sign_field_password_margin_top</item>
        <item name="android:layout_marginEnd">@dimen/sign_field_password_margin_end</item>
        <item name="android:hint">@string/password</item>
    </style>

    <style name="Sign.Button" parent="Widget.AppCompat.Button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">?themeSignButtonsBG</item>
        <item name="android:textColor">?themeSignButtonTextColor</item>
        <item name="android:layout_marginTop">@dimen/sign_field_password_margin_top</item>
        <item name="android:layout_marginEnd">@dimen/sign_field_password_margin_end</item>
    </style>

    <style name="Sign.Button.In">
        <item name="android:backgroundTint">?themeSignButtonInBgColor</item>
        <item name="android:text">@string/sign_in</item>
    </style>

    <style name="Sign.Button.Up">
        <item name="android:backgroundTint">?themeSignButtonUpBgColor</item>
        <item name="android:text">@string/sign_up</item>
    </style>

    <style name="Sign.Progress" parent="Widget.AppCompat.ProgressBar">
        <item name="android:visibility">invisible</item>
        <item name="android:inputType">textPassword</item>
        <item name="android:layout_width">@dimen/sign_progress_bar_size</item>
        <item name="android:layout_height">@dimen/sign_progress_bar_size</item>
    </style>

    <!-- Main Fragment-->
    <style name="Main.Search" parent="Widget.AppCompat.EditText">
        <item name="android:layout_width">@dimen/main_search_field_w</item>
        <item name="android:layout_height">@dimen/main_search_field_h</item>
        <item name="android:textColor">?themeSearchTextColor</item>
        <item name="android:inputType">text</item>
    </style>

    <style name="Main.SearchMotion" parent="Widget.MaterialComponents.TabLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">?themeSignFieldsBG</item>
    </style>

    <style name="SearchPanel.IconButton">
        <item name="android:layout_width">@dimen/main_item_switcher_button_w</item>
        <item name="android:layout_height">@dimen/main_item_switcher_button_h</item>
        <item name="android:layout_margin">@dimen/main_panel_searching_margin</item>
    </style>

    <style name="Main.FAB" parent="Widget.Design.FloatingActionButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">@dimen/main_fab_margin_end</item>
        <item name="android:clickable">true</item>
        <item name="android:foregroundTint">?themeMainFABImageColor</item>
        <item name="android:backgroundTint">?themeMainFABColor</item>
        <item name="maxImageSize">@dimen/main_fab_max_size</item>
        <item name="srcCompat">@drawable/ic_add</item>
        <item name="fabSize">auto</item>
    </style>

    <style name="Main.Buttons" parent="Widget.AppCompat.ImageButton">
        <item name="android:layout_width">@dimen/main_item_switcher_button_w</item>
        <item name="android:layout_height">@dimen/main_item_switcher_button_h</item>
        <item name="android:layout_marginTop">@dimen/main_panel_button_margin_top</item>
        <item name="android:layout_marginStart">@dimen/main_panel_button_margin_start</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:background">@color/transparent</item>
    </style>

    <style name="SearchPanel" parent="Widget.MaterialComponents.TabLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/main_panel_height</item>
        <item name="android:background">?themeSignFieldsBG</item>
        <item name="android:textColor">?themeSearchTextColor</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingStart">@dimen/main_panel_searching_padding_start</item>
        <item name="android:inputType">text</item>
    </style>

    <!-- Creator Fragment-->
    <style name="Creator.Fragment" parent="Widget.MaterialComponents.TabLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:padding">@dimen/item_fragment_layout_padding</item>
    </style>

    <style name="Creator.Name" parent="AppEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:hint">@string/input_name_of_item</item>
    </style>

    <style name="Creator.Description" parent="AppEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:hint">@string/input_description_of_item</item>
    </style>

    <style name="Creator.CloseDate" parent="AppSecondText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/item_fragment_margin_between_view</item>
        <item name="android:text">@string/close_date</item>
        <item name="android:textSize">@dimen/creator_close_date_text_size</item>
        <item name="android:enabled">false</item>
    </style>

    <style name="Creator.CloseDateVal" parent="AppSecondText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/item_fragment_margin_between_blocks</item>
        <item name="android:textSize">@dimen/creator_close_date_text_size</item>
    </style>

    <style name="Creator.CheckBox" parent="AppSecondText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/is_cycle</item>
        <item name="android:layout_marginTop">@dimen/item_fragment_margin_between_view</item>
        <item name="android:textSize">@dimen/creator_close_date_text_size</item>
    </style>

    <style name="Creator.Spinner" parent="AppSecondText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/item_fragment_margin_between_blocks</item>
        <item name="android:textSize">@dimen/creator_close_date_text_size</item>
    </style>

    <style name="Creator.Button" parent="Widget.AppCompat.Button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/item_fragment_margin_between_view</item>
        <item name="android:background">@drawable/bg_sign_button</item>
        <item name="android:textColor">?themeSignButtonTextColor</item>
    </style>

    <!-- Converter -->
    <style name="Converter" parent="Widget.MaterialComponents.TabLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:padding">@dimen/item_fragment_layout_padding</item>
    </style>

    <style name="Converter.Headers" parent="AppMainText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/converter_headers_text_size</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Converter.ContentEdit" parent="AppEditText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/item_fragment_margin_between_view</item>
        <item name="android:layout_marginStart">@dimen/item_fragment_margin_between_blocks</item>
        <item name="android:textSize">@dimen/converter_other_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="Converter.ContentText" parent="AppSecondText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/item_fragment_margin_between_view</item>
        <item name="android:layout_marginStart">@dimen/item_fragment_margin_between_blocks</item>
        <item name="android:textSize">@dimen/converter_other_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="Converter.RadioGroup" parent="AppSecondText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/item_fragment_converter_radio_group_high</item>
        <item name="android:layout_marginTop">@dimen/item_fragment_margin_between_blocks</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:textSize">@dimen/converter_other_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="Converter.RadioItem" parent="AppSecondText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CycleCheck" parent="AppSecondText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:enabled">false</item>
        <item name="android:text">@string/is_cycle</item>
    </style>

    <!-- ItemFragment -->
    <style name="ItemFragment" parent="AppScreen">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ItemFragment.Header" parent="AppMainText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/item_fragment_header1_text_size</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ItemFragment.IsDone" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">@dimen/item_fragment_is_done_size</item>
        <item name="android:layout_height">@dimen/item_fragment_is_done_size</item>
        <item name="android:layout_marginStart">@dimen/item_fragment_small_padding</item>
        <item name="android:layout_marginEnd">@dimen/item_fragment_small_padding</item>
        <item name="android:visibility">invisible</item>
        <item name="android:src">@drawable/done_ph</item>
    </style>

    <style name="ItemFragment.StartDate">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAlignment">textEnd</item>
        <item name="android:layout_marginEnd">@dimen/item_fragment_margin_between_view</item>
        <item name="android:textSize">@dimen/item_fragment_other_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?themeItemFRStartDateColor</item>
    </style>

    <style name="ItemFragment.Header2" parent="AppMainText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/item_fragment_margin_between_blocks</item>
